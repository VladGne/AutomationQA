<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name = "Suite1">
    <listeners>
        <listener class-name="PracticeTestNG.Listener" />
    </listeners>
    <test name = "test1">
        <parameter name = "myPi" value="3.14"/>
        <parameter name = "myE" value="2.71"/>
        <classes>
            <class name = "PracticeTestNG.Task1" />
        </classes>
    </test>
    <test name = "test2">
        <parameter name = "myPi" value="3.14"/>
        <parameter name = "myE" value="2.71"/>
        <groups>
            <run>
                <include name="failTests"/>
                <include name="positiveTests"/>
            </run>
        </groups>
        <classes>
            <class name = "PracticeTestNG.Task1" />
        </classes>
    </test>
</suite>